{
    "env": {
        "browser": true,
        "es2021": true,
        "test": {
            "presets": [[
                "@babel/preset-env",
                {
                "targets": {
                    "node": "current"
                }
                }
            ]]
        }
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0 ,
        "linebreak-style": ["error", "windows"],
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ]
    }
}
